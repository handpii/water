page {
  background-color: #f6f6f6;
  margin: 0rpx 20rpx;
}
.content {
  width: 94%;
  background-color: #ffffff;
}
.title {
  font-size: 50rpx;
  vertical-align: bottom;
}
.title image {
  font-size: 0;
  height: 55rpx;
  vertical-align: bottom;
  width: 55rpx;
}
.contenDetail {
  width: 100%;
}
.contenDetail ._p {
  text-indent: 2em;
  padding: 0px 20rpx;
}
.contenDetail image {
  width: 100%;
  border-top: 15px solid #ffffff;
}
.Textsp {
  font-size: 20rpx;
  color: #ccc;
}

