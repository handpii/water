page {
  background-color: #f6f6f6;
  margin: 0rpx 20rpx;
}
.content {
  width: 94%;
  background-color: #ffffff;
}
swiper {
  width: 100%;
  height: 200px;
}
swiper image {
  width: 100%;
  height: 200px;
}
.kouhao {
  border-top: 10rpx solid #f6f6f6;
  padding-top: 50rpx;
  background-color: #fff;
  width: 100%;
  background-image: url(data:image/jpeg;base64,);
  background-size: 100% 100%;
}
.kouhao .big {
  font-size: 50rpx;
  font-weight: bold;
}
.kouhao .big .bigp {
  color: #18aeff;
}
.kouhao .big .biglogo {
  color: red;
}
.kouhao .min {
  padding-bottom: 30rpx;
}
.activityntroduction {
  display: flex;
  width: 100%;
  padding-top: 50rpx;
  padding-bottom: 20rpx;
  border-top: 10rpx solid #f5f5f5;
  border-buttom: 10rpx solid #f5f5f5;
}
.activityntroduction image {
  flex: 1;
  left: 0px;
  top: 0px;
  width: 150px;
  height: 170px;
  display: block;
}
.activityntroduction .detil {
  flex: 2;
}
.activityntroduction .detil ._h2 {
  text-align: center;
}
.activityntroduction .detil ._p {
  padding-top: 30rpx;
  text-indent: 2em;
  padding: 20rpx 20rpx;
}
.items_icons {
  display: flex;
  align-items: center;
  height: 80px;
  width: 100%;
  background-color: #ffffff;
  border-top: 20rpx solid #f6f6f6;
}
.items_icons .item_icons {
  width: 30%;
  flex: 1;
  text-align: center;
}
.items_icons .item_icons image {
  width: 40px;
  height: 40px;
}

